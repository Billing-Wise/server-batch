      "image": "891376922202.dkr.ecr.ap-northeast-2.amazonaws.com/t5-batch-ecr",
      "cpu": 0,
      "portMappings": [
        {
          "name": "t5-batch-port",
          "containerPort": 9090,
          "hostPort": 9090,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
